{
  "kind": "collectionType",
  "collectionName": "challenges",
  "info": {
    "singularName": "challenge",
    "pluralName": "challenges",
    "displayName": "Challenge",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "userId": {
      "type": "string",
      "required": true,
      "unique": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "name": {
      "type": "string",
      "required": true,
      "unique": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "shortDescription": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "detailDescription": {
      "type": "blocks",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Aim": {
      "type": "blocks",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "potentialSolution": {
      "type": "blocks",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "additionalInformation": {
      "type": "blocks",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "targetAcademicPartnership": {
      "type": "enumeration",
      "enum": [
        "Information Systems",
        "Business Analytics",
        "Cybersecurity",
        "Enterprise Systems",
        "Digital Innovation",
        "Business Process Management",
        "User Experience Design",
        "Data Science",
        "Artificial Intelligence",
        "IT Governance",
        "Business Intelligence",
        "Cloud Computing",
        "Digital Transformation",
        "Systems Integration",
        "Blockchain & Digital Assets",
        "Information Management",
        "Technology Strategy",
        "Decision Support Systems",
        "Metaverse Technologies",
        "IT Service Management"
      ]
    },
    "studentLevel": {
      "type": "enumeration",
      "enum": [
        "Undergraduate 1st & 2nd year",
        "Undergraduate penultimate & final year",
        "Postgraduate",
        "Other"
      ]
    },
    "startDate": {
      "type": "date",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "endDate": {
      "type": "date"
    },
    "isActive": {
      "type": "boolean",
      "default": true
    },
    "challengeStatus": {
      "type": "enumeration",
      "enum": [
        "upcoming",
        "ongoing",
        "complete"
      ]
    },
    "country": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "multimedia": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "companyName": {
      "type": "string"
    },
    "isPartnered": {
      "type": "boolean"
    }
  }
}
